    "eslint-plugin-jest": "^23.8.2",
    "jest": "^22.1.1",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.1",
    "validate-commit": "^3.4.0"
  },
  "dependencies": {
